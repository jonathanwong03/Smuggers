{
  "name": "tempo-backend",
  "version": "1.0.0",
  "description": "Backend for NASA TEMPO Air Quality Forecast App",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "migrate": "node scripts/loadCSVData.js",
    "db:reset": "rm -f data/air_quality.db && npm run migrate",
    "db:view": "node scripts/viewDatabase.js",
    "db:web": "node scripts/webViewer.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.21.2",
    "sqlite3": "^5.1.6",
    "csv-parser": "^3.0.0"
  },
  "devDependencies": {
    "@types/leaflet": "^1.9.20",
    "nodemon": "^2.0.22"
  }
}
